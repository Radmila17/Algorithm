    "def strFight(s1, s2):\n",
    "    n = len(s1)\n",
    "    s1 = ''.join(sorted(s1))\n",
    "    s2 = ''.join(sorted(s2))\n",
    "    print(s1, s2)\n",
    "    count1 = 0\n",
    "    count2 = 0\n",
    "    for i in range(n):\n",
    "        if s1[i] > s2[i]:\n",
    "            count1+=1\n",
    "        elif s1[i] < s2[i]:\n",
    "            count2+=1\n",
    "        else:\n",
    "            count1+=1\n",
    "            count2+=1\n",
    "    if  count1 == n or  count2 == n:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "    \n",
    "print(battle_strings(\"abc\", \"xya\"))\n",
    "print (\"\")\n",
    "print(battle_strings(\"abe\", \"acd\"))"
   ]
